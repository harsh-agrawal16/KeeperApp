        {
          "name": "Get Point Of Attractions",
          "event": [
            {
              "listen": "test",
              "script": {
                "id": "aa3a20e8-c6e6-4f1e-bd56-a72237f23b1f",
                "exec": [
                  "if(pm.response.code === 200){",
                  "    let locations = pm.response.json().data",
                  "    let pois = []",
                  "    for(var i=0;i<locations.length;i++){",
                  "        let poi = locations[i]",
                  "        pois.push(poi.name)",
                  "        pm.environment.set('message', poi.name)",
                  "        postman.setNextRequest('Send Message')",
                  "    }",
                  "",
                  "    pm.environment.set('poi', pois[0])",
                  "    // console.log(pois)",
                  "    // pm.environment.set('message', pois.toString())",
                  "    // console.log(pois.toString())",
                  "}"
                ],
                "type": "text/javascript"
              }
